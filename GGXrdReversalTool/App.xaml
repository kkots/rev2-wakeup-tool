<Application x:Class="GGXrdReversalTool.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:GGXrdReversalTool"
             xmlns:converters="clr-namespace:GGXrdReversalTool.Converters"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <converters:PercentageFrequencyConverter x:Key="PercentageFrequencyConverter" />
        <converters:RandomSlotHeaderConverter x:Key="RandomSlotHeaderConverter" />
        <converters:InputConverter x:Key="InputConverter" />
        <converters:InputSplitConverter x:Key="InputSplitConverter" />
        <converters:AntiBooleanConverter x:Key="AntiBooleanConverter" />
        <converters:VisibilityCollapsedConverter x:Key="VisibilityCollapsedConverter"/>
        <converters:AntiVisibilityCollapsedConverter x:Key="AntiVisibilityCollapsedConverter"/>
    </Application.Resources>
</Application>
